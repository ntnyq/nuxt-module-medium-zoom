{
  "name": "@nuxt-dev/medium-zoom",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@10.13.1",
  "description": "MediumZoom integrated for Nuxt",
  "keywords": [
    "image",
    "medium-zoom",
    "nuxt",
    "nuxt-module",
    "nuxt3"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://nuxt-module-medium-zoom.ntnyq.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ntnyq/nuxt-module-medium-zoom.git"
  },
  "bugs": {
    "url": "https://github.com/ntnyq/nuxt-module-medium-zoom/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/types.d.mts",
      "default": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "types": "./dist/types.d.mts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "nuxt-module-build build",
    "clean": "premove dist .turbo .nuxt",
    "deploy": "run-s dev:prepare build play:build",
    "dev": "nuxi dev playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "lint": "eslint",
    "play:build": "nuxi generate playground",
    "play:dev": "nuxi dev playground",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:version release:publish",
    "release:check": "run-s lint typecheck test",
    "release:publish": "pnpm publish --access public",
    "release:version": "bumpp",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "catalog:",
    "defu": "^6.1.4",
    "medium-zoom": "catalog:"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^5.2.0",
    "@ntnyq/prettier-config": "^3.0.1",
    "@nuxt/eslint-config": "^1.7.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "catalog:",
    "@types/node": "^24.1.0",
    "bumpp": "^10.2.0",
    "eslint": "^9.32.0",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^8.0.4",
    "nuxt": "catalog:",
    "premove": "^4.0.0",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "vitest": "catalog:"
  },
  "nano-staged": {
    "*.{js,ts,mjs,cjs,vue,md,svg,yml,yaml,json}": "eslint --fix",
    "*.{css,scss,html}": "prettier -uw"
  }
}
