{
  "name": "@nuxt-dev/medium-zoom",
  "type": "module",
  "version": "0.0.2",
  "packageManager": "pnpm@10.6.3",
  "description": "MediumZoom integrated for Nuxt",
  "keywords": [
    "image",
    "medium-zoom",
    "nuxt",
    "nuxt-module",
    "nuxt3"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://nuxt-module-medium-zoom.ntnyq.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ntnyq/nuxt-module-medium-zoom.git"
  },
  "bugs": {
    "url": "https://github.com/ntnyq/nuxt-module-medium-zoom/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/types.d.mts",
        "default": "./dist/module.mjs"
      },
      "require": {
        "types": "./dist/types.d.ts",
        "default": "./dist/module.cjs"
      }
    }
  },
  "main": "./dist/module.cjs",
  "module": "./dist/module.mjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "nuxt-module-build build",
    "clean": "premove dist .turbo .nuxt",
    "coverage": "vitest --coverage",
    "deploy": "run-s dev:prepare build play:build",
    "dev": "nuxi dev playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "lint": "eslint .",
    "play:build": "nuxi generate playground",
    "play:dev": "nuxi dev playground",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:version release:publish",
    "release:check": "run-s lint typecheck test",
    "release:publish": "pnpm publish --access public",
    "release:version": "bumpp",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "catalog:",
    "defu": "^6.1.4",
    "medium-zoom": "catalog:"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^4.0.2",
    "@ntnyq/prettier-config": "^2.0.0",
    "@nuxt/eslint-config": "^1.2.0",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "catalog:",
    "@types/node": "^22.13.10",
    "@vitest/coverage-v8": "catalog:",
    "bumpp": "^10.1.0",
    "eslint": "^9.22.0",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^7.0.2",
    "nuxt": "catalog:",
    "premove": "^4.0.0",
    "prettier": "^3.5.3",
    "tsx": "^4.19.3",
    "typescript": "~5.6.3",
    "vitest": "catalog:"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ],
    "overrides": {
      "esbuild": "^0.25.1"
    }
  },
  "nano-staged": {
    "*.{js,ts,mjs,cjs,vue,json,md,yml,yaml,svg}": "eslint --fix",
    "*.{css,scss,html}": "prettier -uw"
  }
}
